  } else {
    Serial.println("Failed to push aqi value: " + fbdo.errorReason());
   } 
  // Read the button state
  if (buttonPressed) {
    buttonPressed = false; // Reset the flag
    programNumber++; // Increment program number
    if (programNumber >= maxPrograms) {
      programNumber = 0; // Wrap around
    }
    Serial.print("Switched to program: ");
